{
  "name": "dwarfium",
  "version": "2.5.4",
  "private": true,
  "pkg": {
    "scripts": [
      "./server/server.js",
      "./node_modules/express/**/*",
      "./node_modules/node-fetch/**/*",
      "./node_modules/cors/**/*"
    ]
  },
  "scripts": {
    "dev": "next dev -H 0.0.0.0",
    "dev:tauri": "next dev",
    "dev:windows": "concurrently \"cd ./install/windows && mediamtx.exe ..\\config\\mediamtx.yml\" \"npm run dev",
    "dev:linux": "concurrently \"cd ./install/linux && ./mediamtx ../config/mediamtx.yml\" \"npm run dev",
    "build": "next build",
    "build:api": "cross-env NEXT_PUBLIC_URL_PROXY_CORS=/api/proxy next build",
    "start:api": "cross-env NEXT_PUBLIC_URL_PROXY_CORS=/api/proxy next start",
    "start:windows": "concurrently \"cd ./install/windows && mediamtx.exe ..\\config\\mediamtx.yml\" \"npm run start:api\"",
    "start:linux": "concurrently \"cd ./install/linux && ./mediamtx ../config/mediamtx.yml\" \"npm run start:api\"",
    "start:macos": "concurrently \"cd ./install/macos && ./mediamtx ../config/mediamtx.yml\" \"npm run start:api\"",
    "build:server": "npx tsx ./server/build-server.ts  && npx tsx ./server/build-SSLcert.ts",
    "package:server": "node ./scripts/package-server.js",
    "copy:tools": "node ./scripts/copy-tools.js",
    "postbuild": "npm run build:server && npm run package:server && npm run copy:tools",
    "tauri": "node scripts/set-version.js && tauri",
    "zipsite": "npm run build && npm run prepare:dist && tar -czvf Dwarfium.tar.gz Dwarfium && rm -rf Dwarfium",
    "zipsite:win": "npm run build && npm run prepare:dist && powershell -Command \"Compress-Archive -Path Dwarfium -DestinationPath Dwarfium.zip; Remove-Item -Recurse -Force Dwarfium\"",
    "zipsite:wsl": "cross-env NEXT_PUBLIC_URL_PROXY_CORS=http://$(hostname -I):8860,NEXT_PUBLIC_IP_MEDIAMTX=$(hostname -I) npm run build && npm run prepare:dist && tar -czvf Dwarfium.tar.gz Dwarfium && rm -rf Dwarfium",
    "ziptools:win": "npm run postbuild && npm run prepare:tools && powershell -Command \"Compress-Archive -Path DwarfiumProxy -DestinationPath DwarfiumProxy.zip; Remove-Item -Recurse -Force DwarfiumProxy\"",
    "zipserver:win": "npm run build && npm run prepare:server && powershell -Command \"Compress-Archive -Path DwarfiumServer -DestinationPath DwarfiumServer.zip; Remove-Item -Recurse -Force DwarfiumServer\"",
    "prepare:dist": "node scripts/prepareDist.js",
    "prepare:tools": "node scripts/prepareProxy.js",
    "prepare:server": "node scripts/prepareServer.js",
    "certificate":"npx tsx ./server/createSSLcert.ts \"./cert\" ",
    "lint": "next lint",
    "format": "prettier --check \"src/**/*.{ts,tsx,js,jsx}\" \"server/*.ts\"",
    "format:fix": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\" \"server/*.ts\"",
    "typecheck": "tsc --noEmit",
    "CI": "npm run lint && npm run format && npm run typecheck && npm run test",
    "test": "jest",
    "test:watch": "jest . --watch"
  },
